{
  "name": "@issos1993/flow-random",
  "version": "1.1.5",
  "description": "A library that help to flow control by a random number",
  "main": "index.js",
  "scripts": {
    "test": "jest --watchAll",
    "build": "tsc",
    "start": "tsc && node ./updateImports.js && http-server -p 8080",
    "lint": "eslint ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/barbosalucas278/flow-random-js.git"
  },
  "keywords": [
    "random",
    "flow",
    "javascript",
    "probability"
  ],
  "type": "module",
  "author": "Lucas Barbosa <barbosalucasroberto@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/barbosalucas278/flow-random-js/issues"
  },
  "homepage": "https://github.com/barbosalucas278/flow-random-js#readme",
  "devDependencies": {
    "@babel/cli": "^7.15.4",
    "@babel/core": "^7.23.6",
    "@babel/preset-env": "^7.23.6",
    "@babel/preset-typescript": "^7.23.3",
    "@jest/globals": "^29.7.0",
    "babel-jest": "^29.7.0",
    "eslint": "^8.2.0",
    "http-server": "^14.1.1",
    "prettier": "^2.5.1",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  },
  "jest": {
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
